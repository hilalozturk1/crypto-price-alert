    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "mongoose": "^8.3.2",
    "node-cron": "^4.1.0",
    "nodemon": "^3.1.10",
    "prom-client": "^15.1.3",
    "swagger-jsdoc": "^6.2.8",
